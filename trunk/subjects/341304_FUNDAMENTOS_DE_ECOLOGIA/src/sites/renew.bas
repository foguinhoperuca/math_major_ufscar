10 REM IBM: RENEW
15 REM GROWTH ON A RENEWABLE CONSTANT-FLOW SOURCE
20 CLS
25 SCREEN 1, 0: COLOR 15, 0
27 LINE (0, 0)-(319, 180), 0, BF
30 LINE (0, 0)-(319, 180), 1, B
40 J = 45
50 Q = .1
60 K = .1
70 K3 = 8.000001E-03
80 K4 = .03
100 PSET (T, 180 - Q), 2
110 JR = J / (1 + K * Q)
120 DQ = K3 * JR * Q - K4 * Q
130 Q = Q + DQ
140 T = T + 1
150  IF T < 319 GOTO 100
160 LOCATE 2, 2: PRINT "Renew.bas"
170 SLEEP: SLEEP: END

